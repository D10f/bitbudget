{
  "name": "gadget_budget",
  "version": "1.0.0",
  "description": "Expense tracker application",
  "main": "index.js",
  "scripts": {
    "start": "env-cmd -f ./config/dev.env node index.js",
    "serve": "env-cmd -f ./config/dev.env nodemon index.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run serve\" \"npm run client\"",
    "test": "env-cmd -f ./config/test.env jest --watch --runInBand"
  },
  "jest": {
    "watchPathIgnorePatterns": [
      "node_modules"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "argon2": "^0.28.2",
    "bcryptjs": "^2.4.3",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.11.15",
    "numeral": "^2.0.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
    "jest": "^27.0.5",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3"
  }
}
